{
  "name": "decisiontree",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env dist/src/index.js",
    "compile": "npx tsc --project tsconfig.json",
    "compile-watch": "npx tsc --project tsconfig.json --watch",
    "lint": "npx eslint --config eslint.config.js",
    "prettify": "npx prettier . --write",
    "vitest": "vitest run --coverage --silent",
    "vitest-with-logs": "vitest run --coverage",
    "vitest-watch": "vitest --coverage --silent",
    "create-doc": "npx apidoc -i src -o apidoc --file-filters .mts",
    "docker-build": "docker build -f Dockerfile --progress=plain -t decisiontree:latest .",
    "docker-run": "docker run --env-file .env -p 3000:3000 decisiontree:latest",
    "docker-tag": "docker image tag decisiontree:latest geoimpact.azurecr.io/decisiontree:latest",
    "docker-push": "docker image push geoimpact.azurecr.io/decisiontree:latest"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/pg": "^8.11.6",
    "@types/yargs": "^17.0.33",
    "axios": "^1.7.2",
    "jsonwebtoken": "^9.0.2",
    "koa": "~2.14.1",
    "node-jose": "^2.2.0",
    "pg": "^8.12.0",
    "puppeteer": "^23.2.2",
    "xml2js": "^0.6.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "@types/koa": "~2.13.5",
    "@types/node": "~18.16.9",
    "@types/node-jose": "^1.1.13",
    "@types/xml2js": "^0.4.14",
    "@vitest/coverage-v8": "^2.0.5",
    "apidoc": "^1.2.0",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.7.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vitest": "^2.0.5"
  }
}
